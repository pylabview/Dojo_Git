for (var i = 1; i<20;i++){
    if(i%2 !==0){
        console.log(i);
    }
}

for (var i= 100; i>0; i--){
    if(i%3 == 0){
        console.log(i);
    }
}

a = [4, 2.5, 1, -0.5, -2, -3.5]

for (var i =0; i < a.length; i++){
    console.log(a[i])
}

function sigma(){
    var sum = 0;
    for (var i=1; i < 101; i++){
        sum= sum +i;
        }
    return sum;
}

console.log(sigma())

function factorial(){
    var product = 1
    for(i=1; i<13; i++){
        product = product*i;
    }
    return product;
}

console.log(factorial())